{
  "arrowParens": "always",
  "bracketSameLine": false,
  "bracketSpacing": true,
  "endOfLine": "lf",
  "jsxSingleQuote": false,
  "quoteProps": "as-needed",
  "semi": true,
  "singleAttributePerLine": true,
  "singleQuote": true,
  "tabWidth": 2,
  "printWidth": 80,
  "trailingComma": "es5",
  "useTabs": false,
  "overrides": [
    {
      "files": "*.js",
      "options": {
        "tabWidth": 2,
        "printWidth": 80,
        "useTabs": false,
        "indent": 2,
        "ignorePath": ".prettierignore"
      }
    }
  ]
}
