{
"env": {
  "browser": true,
  "es2021": true,
  "node": true
},
"extends": [
  "airbnb",
  "prettier",
  "eslint:recommended"
],
"overrides": [],
"parserOptions": {
  "ecmaVersion": "latest",
  "sourceType": "module"
},
"plugins": [
  "prettier"
],
"rules": {
  "indent": ["error", 2],
  "linebreak-style": ["error", "unix"],
  "semi": ["error", "always"],
  "no-console": "off",
  "no-unused-vars": "warn",
  "no-nested-ternary": "off",
  "react/jsx-uses-react": "warn",
  "react/jsx-uses-vars": "warn",
  "prettier/prettier": "error",
  "no-underscore-dangle": "off"
}
}
